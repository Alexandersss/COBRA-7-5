
clean:
	rm -f *DEBUG *.o *.elf *.self *.484 *.bin *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o

all:
	rm -f *.o *.elf *.self *.484 *.bin *.484c *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o
	make -f Makefile_484C --no-print-directory

	rm -f *.o *.elf *.484 *.bin *.484c *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o
	make -f Makefile_484C_dbg --no-print-directory

	rm -f *.o *.elf  *.484 *.bin *.484c *.484d *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o

dbg:
	rm -f *.o *.elf *.self *.484 *.bin *.484c *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o
	make -f Makefile_484C_dbg --no-print-directory

	rm -f *.o *.elf  *.484 *.bin *.484c *.484d *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o

re:
	rm -f *.o *.elf *.self *.484 *.bin *.484c *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o
	make -f Makefile_484C --no-print-directory

	rm -f *.o *.elf  *.484 *.bin *.484c *.484d *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o
